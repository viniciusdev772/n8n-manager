# Token de autenticação da API (obrigatório — gere com: python3 -c "import secrets; print(secrets.token_hex(32))")
API_AUTH_TOKEN=

# Domínio base para instâncias (ex: nome.seudominio.com)
BASE_DOMAIN=n8n.seudominio.com

# Email para certificados SSL (Let's Encrypt via Traefik)
ACME_EMAIL=admin@seudominio.com

# Cloudflare DNS API Token (obrigatório para DNS Challenge do Traefik)
CF_DNS_API_TOKEN=

# RabbitMQ
RABBITMQ_HOST=127.0.0.1
RABBITMQ_PORT=5672
RABBITMQ_USER=n8n_manager
RABBITMQ_PASSWORD=senha_segura_aqui

# Redis
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

# Rede Docker para Traefik + instâncias
DOCKER_NETWORK=n8n-public

# Porta do servidor FastAPI
SERVER_PORT=5050

# CORS — origens permitidas (separadas por vírgula, ou * para todas)
ALLOWED_ORIGINS=*

# Cleanup automático
CLEANUP_MAX_AGE_DAYS=5
CLEANUP_INTERVAL_SECONDS=3600

# Timezone padrão para instâncias N8N
DEFAULT_TIMEZONE=America/Sao_Paulo

# Job status TTL (Redis)
JOB_TTL=600
JOB_CLEANUP_TTL=300

# Parser API dedicada (subdomínio exclusivo)
PARSER_SUBDOMAIN=parser
